<?xml version="1.0" encoding="UTF-8"?>
<component name="LanguageInjectionConfiguration">
    <injection language="SQL" injector-id="xml">
        <display-name>SqlToy Sql</display-name>
        <single-file />
        <place><![CDATA[
            xmlTag().withLocalName("value").withParent(xmlTag().withLocalName("sql").withParent(xmlTag().withLocalName("sqltoy")))
                    .inVirtualFile(virtualFile().withName(string().endsWith("sql.xml")))
            ]]>
        </place>
    </injection>

    <injection language="jvm-class-name" injector-id="java">
        <display-name>idGenerator Class</display-name>
        <single-file />
        <place><![CDATA[
            psiMethod().withName("generator").withParameters().definedInClass("org.sagacity.sqltoy.config.annotation.Id")
            ]]>
        </place>
    </injection>

    <injection language="SQL" injector-id="java">
        <display-name>Mapping Load Sql</display-name>
        <single-file />
        <place><![CDATA[
            psiMethod().withName("load").withParameters().definedInClass("org.sagacity.sqltoy.config.annotation.OneToMany")
            ]]>
        </place>
        <place><![CDATA[
            psiMethod().withName("load").withParameters().definedInClass("org.sagacity.sqltoy.config.annotation.OneToOne")
            ]]>
        </place>
    </injection>
</component>